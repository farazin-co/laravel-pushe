{
    "name": "novaday-co/laravel-pushe",
    "description": "Send Pushe notification in Laravel",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/novaday-co/laravel-pushe",
    "authors": [
        {
            "name": "Farid",
            "email": "froozan@yahoo.com",
            "homepage": "https://faridfr.ir"
        }
    ],
    "require": {
        "illuminate/http": "~5.0|~6.0|~7.0|~8.0",
        "illuminate/support": "^8.0|^7.0|^6.0|^5.5",
        "guzzlehttp/guzzle": "^7.4@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "NovadayCo\\LaravelPushe\\": "src"
        }
    },
    "autoload-dev": {
        "NovadayCo\\LaravelPushe\\": "src",
        "classmap": [
            "tests"
        ]
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovadayCo\\LaravelPushe\\PusheServiceProvider"
            ]
        }
    }
}
